{
  "name": "docs",
  "version": "5.15.15",
  "private": true,
  "scripts": {
    "dev": "./scripts/now-examples-fetch.sh && next",
    "build": "./scripts/now-examples-fetch.sh && cross-env NODE_ENV=production next build",
    "now-build": "yarn run build && yarn run export",
    "now-dev": "./scripts/now-examples-fetch.sh && next -p $PORT",
    "start": "cross-env NODE_ENV=production next start",
    "export": "next export -o dist",
    "lint": "eslint .",
    "test": "yarn run lint",
    "update-mdx-meta": "node scripts/update-mdx-meta",
    "prettier": "prettier --write \"**/*.{js,json,md,mdx}\"",
    "docs-index": "node ./scripts/index-docs.js"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "0.3.0",
    "@mdx-js/loader": "0.15.5",
    "@mdx-js/mdx": "0.15.5",
    "@mdx-js/tag": "0.15.0",
    "@sindresorhus/slugify": "0.6.0",
    "@skidding/react-codemirror": "1.0.2",
    "@zeit/next-mdx": "1.2.0",
    "algoliasearch": "3.32.1",
    "async-retry": "1.2.3",
    "babel-plugin-root-import": "6.1.0",
    "babel-plugin-transform-define": "1.3.1",
    "cheerio": "1.0.0-rc.2",
    "codemirror": "5.31.0",
    "content-type": "1.0.4",
    "cross-env": "5.2.0",
    "date-fns": "1.29.0",
    "dateformat": "3.0.3",
    "extract-mdx-metadata": "1.0.0",
    "fs-extra": "7.0.1",
    "git-jiggy": "1.0.1",
    "globby": "9.1.0",
    "intersection-observer": "0.5.0",
    "isomorphic-unfetch": "2.0.0",
    "js-cookie": "2.2.0",
    "lodash.debounce": "4.0.8",
    "md5": "2.2.1",
    "mitt": "1.1.2",
    "next": "8.1.1-canary.37",
    "nprogress": "0.2.0",
    "prop-types": "15.6.2",
    "qs": "6.6.0",
    "ramda": "0.25.0",
    "react": "16.8.4",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.8.4",
    "react-instantsearch-dom": "5.4.0",
    "react-markdown": "4.0.6",
    "refractor": "2.8.0",
    "rehype": "7.0.0",
    "remark": "10.0.1",
    "remark-mdx": "0.20.3",
    "remark-mdx-metadata": "1.0.1",
    "remark-stringify": "6.0.4",
    "scroll-into-view-if-needed": "1.1.0",
    "to-vfile": "5.0.2",
    "unist-util-filter": "1.0.0",
    "unist-util-find-all-after": "1.0.2",
    "unist-util-find-all-before": "2.0.2",
    "unist-util-find-all-between": "1.0.2",
    "unist-util-map": "^1.0.4",
    "unist-util-select": "1.5.0",
    "unist-util-visit": "1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "eslint": "4.2.0",
    "eslint-plugin-import-b5a962": "2.3.0",
    "eslint-plugin-react": "7.1.0",
    "execa": "1.0.0",
    "husky": "1.2.1",
    "lint-staged": "8.1.0",
    "prettier": "1.16.1"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "babel-plugin-root-import"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "yarn run update-mdx-meta -- post-commit"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint"
    ],
    "*.{js,json,md,mdx}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "experimentalObjectRestSpread": true
      },
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "react",
      "import-b5a962"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "func-names": [
        "error",
        "as-needed"
      ],
      "no-shadow": "error",
      "import-b5a962/no-mutable-exports": "error",
      "import-b5a962/no-anonymous-default-export": [
        "error",
        {
          "allowArray": true,
          "allowArrowFunction": true,
          "allowLiteral": true,
          "allowObject": true
        }
      ],
      "no-extra-semi": 0,
      "no-useless-escape": 0,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "react/no-unescaped-entities": 0,
      "react/jsx-no-target-blank": 0,
      "react/no-string-refs": 0
    }
  }
}
